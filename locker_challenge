import java.util.Scanner;

public class Challenge {

	public static int lockerCheck(int numberOfLockers) {
		int result = 0;
		int delta = 0;
		boolean[] lockers = new boolean[numberOfLockers];
		boolean isOpen = true;
		while (delta < numberOfLockers) {
			for (int i = 0; i < numberOfLockers; i++) {
				if ((i + 1) % (delta+1) == 0) {
					if (lockers[i] == isOpen) {
						lockers[i] = !isOpen;
					} else {
						lockers[i] = isOpen;
					}
				}
				System.out.print(lockers[i]+" ");
				if (i == numberOfLockers - 1) {
					delta++;
				}
				
			}
			System.out.println("\n"+delta);
		}
		for(int i=0;i<numberOfLockers;i++) {
			if (lockers[i]==isOpen) {
				result++;
			}
			
		}
		System.out.println("\n"+result);
		return result;
	}
